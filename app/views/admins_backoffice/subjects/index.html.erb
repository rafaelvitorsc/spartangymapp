<div class="row">
<div class="col-lg-12">
<h1 class="page-header"><%= t('messages.listing', model: @subjects.model_name.human(count: @subjects.count)) %></h1>
</div>
</div>

<table class="table table-dark table-hover">
<thead>
<tr>
<th scope="col"><%= translate_attribute(@subjects, :id) %></th>
<th scope="col"><%= translate_attribute(@subjects, :description) %></th>
<th scope="col">
<%= link_to "Novo", new_admins_backoffice_subject_path, class:"btn btn-success" %>
</th>
</tr>
</thead>
<tbody>
<% @subjects.each do |subject| %>
<tr>
<th scope="row"><%= subject.id %></th>
<td><%= subject.description %></td>
<td style="width:100px">
<div class="btn-group" role="group" aria-label="Basic mixed styles example">
<%= link_to "Editar", edit_admins_backoffice_subject_path(subject), class:"btn btn-primary" %>
<%= link_to "Excluir", admins_backoffice_subject_path(subject), data: { turbo_method: :delete }, class:"btn btn-danger" %>
</div>
</td>
</tr>
<% end %>
</tbody>
</table>

<%= paginate @subjects%>

