<div class="row">
<div class="col-lg-12">
<h1 class="page-header"><%= t('messages.listing', model: @frequencies.model_name.human(count: @frequencies.count)) %></h1>
</div>
</div>

<table class="table table-dark table-hover">
<thead>
<tr>
<th scope="col"><%= translate_attribute(@frequencies, :data) %></th>
<th scope="col"><%= translate_attribute(@frequencies, :turno) %></th>
<th scope="col"><%= translate_attribute(@frequencies, :treino) %></th>

<th scope="col">
<%= link_to "Voltar", users_backoffice_welcome_index_path, class:"btn btn-success" %>

</th>
</tr>
</thead>
<tbody>
<% @frequencies.each do |frequency| %>
<tr>
<th scope="row"><%= frequency.data %></th>
<td><%= frequency.turno %></td>
<td><%= frequency.treino %></td>

<td style="width:100px">
<div class="btn-group" role="group" aria-label="Basic mixed styles example">
<%= link_to "Visualizar", edit_users_backoffice_frequency_path(frequency), class:"btn btn-primary" %>

</div>
</td>
</tr>
<% end %>
</tbody>
</table>

<%= paginate @frequencies%>


