<div class="row">
<div class="col-lg-12">
<h1 class="page-header"><%= t('messages.listing', model: @exams.model_name.human(count: @exams.count)) %></h1>
</div>
</div>

<table class="table table-dark table-hover">
<thead>
<tr>
<th scope="col"><%= translate_attribute(@exams, :id) %></th>
<th scope="col"><%= translate_attribute(@exams, :date) %></th>
<th scope="col"><%= translate_attribute(@exams, :description) %></th>

<th scope="col">
<%= link_to "Voltar", users_backoffice_welcome_index_path, class:"btn btn-success" %>

</th>
</tr>
</thead>
<tbody>
<% @exams.each do |exam| %>
<tr>
<th scope="row"><%= exam.id %></th>
<td><%= exam.date %></td>
<td><%= exam.description %></td>

<td style="width:100px">
<div class="btn-group" role="group" aria-label="Basic mixed styles example">
<%= link_to "Visualizar", edit_users_backoffice_exam_path(exam), class:"btn btn-primary" %>

</div>
</td>
</tr>
<% end %>
</tbody>
</table>

<%= paginate @exams%>


